<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.am.mdrr">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".mdsimple.SnackbarActivity"></activity>
        <activity android:name=".mdsimple.CardViewActivity"/>
        <activity android:name=".mdsimple.TextInputActivity"/>
        <activity android:name=".mdsimple.FloatingActionButtonActivity"/>
        <activity android:name=".mdsimple.ToolbarActivity"/>
        <activity android:name=".mdsimple.ThreeOneActivity"/>
        <activity android:name=".mdsimple.TabLayoutActivity"/>
    </application>

</manifest>